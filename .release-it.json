{
  "git": {
    "commitMessage": "packs-sdk: release v${version}"
  },
  "github": {
    "release": true
  },
  "plugins": {
    "@release-it/keep-a-changelog": {
      "filename": "CHANGELOG.md",
      "addUnreleased": true,
      "addVersionUrl": true,
      "versionUrlFormats": {
        "repositoryUrl": "https://github.com/coda/packs-sdk"
      }
    }
  }
}
