{
  // Version of the setting file.  Always 0.2.
  "version": "0.2",
  "language": "en",
  // List of additional words to add to the dictionary.
  "words": [
    "Codans",
    "automations",
    "Precanned",
    "embeddable",
    "vararg",
    "templating",
    "typeof",
    "PKCE",
    "prepopulated",
    "Iframely",
    "sandboxed",
    "sandboxing",
    "Asana"
  ],
  "ignoreRegExpList": [
    // Code blocks.
    "```(.|\n)*```",
    // Inline code.
    "`.*`",
    // Python macros.
    "{%.*%}",
    // HTML tags.
    "<.*>",
    // Icon references.
    ":.*:",
    // Icons in page metadata.
    "^icon: .*"
  ]
}
